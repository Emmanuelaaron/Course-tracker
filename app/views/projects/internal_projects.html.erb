<header class="profile d-flex justify-content-center pt-5 pb-5 flex-column align-items-center">
    <img src="data:image/png;base64,">
    <%if logged_in?%>
        <h5 class="mt-5"><%=current_user.username%></h5>
    <%end%>
</header>

<div class="projects-display pt-5">
    <%@internalprojects.first.all.each do |project|%>
        <div class='row, justify-content-between'>
            <div class="profile_content bg-light mr-3 ml-3 d-flex justify-content-between pl-4 pr-4 align-items-center mb-4">
                <div>
                    <img src=<%=url_for(project.coursemodules.first.avatar)%> class='col-2'>
                </div>
                <%=project.name%>
            </div>
        </div>
    <%end%>
</div>
<footer class="d-flex align-items-center justify-content-center">
    <span class="border border-white span-footer">
        <%= link_to 'Add new Course', new_coursemodule_path%>
    </span>
</footer>

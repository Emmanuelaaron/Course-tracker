<header class="row">
    <div class="d-flex  flex-row justify-content-around pt-5 pb-5 col-12 proj">
        <div class=""><i class="fa fa-bars"></i></div>
        <div>PROJECTS</div>
        <div><i class="fa fa-search"></i></div>
    </div>
    <div class="col-12 d-flex most-recent justify-content-end  mr-5 pr-5">
        <div>Most recent</div>
        <div class="ml-5">Most acient</div>
    </div>
</header>
<div class="d-flex days flex-row justify-content-between bg-light col-12 py-5">
    <div>Total days</div>
    <div><%=@days%></div>
</div>
<% @projects.each do |project|%>
    <ul>
        <li><%=project.name%>, <%=project.amount%> days</li>
    </ul>
    
<%end%>
<span>
    <%= link_to 'Add Project', new_project_path%>
</span>
